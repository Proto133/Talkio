<div class="container">
  <div class="card">
    <div class="activator card-image waves-effect waves-block waves-light">
      <img class="activator" src="/images/signup.png">
    </div>
    <div class="card-content">
      <span class="card-title activator grey-text text-darken-4">Sign Up<i class="material-icons right">more_vert</i></span>
    </div>
    <div class="card-reveal">
      <span class="card-title grey-text text-darken-4 cardTitle">Create a new account<i class="right">Go Back</i></span>
       <div class="row">
    <form class="col s12">
       <div class="row">
        <div class="input-field col s10">
          <input placeholder="John" id="nameSignUp" type="text">
          <label for="nameSignUp">Name</label>
        </div>
      </div>

       <div class="row">
        <div class="input-field col s10">
          <input placeholder="GITHUB USERNAME" id="github" type="text">
          <label for="github">Github Username</label>
        </div>
      </div>
       <div class="row">
        <div class="input-field col s10">
          <input placeholder="JohnnyDOE" id="userNameSignUp" type="text">
          <label for="userNameSignUp">Username</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s10">
          <input placeholder="email@domain.com" id="emailSignUp" type="email" class="validate">
          <label for="emailSignUp">Email</label>
        </div>
      </div>
     <div class="row">
        <div class="input-field col s10">
          <input id="passSignUp" type="password" class="validate">
          <label for="passSignUp">Password</label>
        </div>
      </div>
      <div class="row">
        <button id="submitSignUp" class="btn waves-effect waves-light" type="button" name="action">Submit
    <i class="material-icons right">send</i>
  </button>
       </div>
  </div>
    </div>
</div>
    <br>
  <div class="row">
  <a href="/login" id="loginBtn" class="btn submitBtn waves-effect waves-light" type="button">Login
    <i class="material-icons right">lock_open</i>
  </a>
  </div>
  </div>

<script src="/js/signup.js"></script>
<script src="/js/materialize.min.js"></script>